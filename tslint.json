{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-consecutive-blank-lines": false,
        "no-string-literal": false,
        "no-var-requires": true,
        "object-literal-sort-keys": false,
        "no-console": [
            true,
            "log"
        ],
        "no-trailing-whitespace": false,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "variable-name": [
            true,
            "allow-leading-underscore"
        ],
        "ordered-imports": false,
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": [
            true,
            {
                "singleline": "never",
                "multiline": "never"
            }
        ],
        "max-line-length": [
            140
        ]
    }
}
