{
  "name": "cub-pack-site",
  "description": "An open source project to let any cub scout pack quickly build an online presence.",
  "version": "1.0.0-0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robdesisto/cub-pack-site.git"
  },
  "keywords": [
    "cub scouts",
    "scouting"
  ],
  "author": "Rob DeSisto",
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack --config build/webpack.prod.js",
    "lint": "npm run lint:style && npm run lint:ts",
    "lint:style": "stylelint \"src/**/*.(scss|vue)\"",
    "lint:ts": "tslint \"src/**/*.ts\"",
    "release": "standard-version --prerelease && node scripts/push-tag.js",
    "start": "webpack-dev-server --hot --progress --port 8122"
  },
  "dependencies": {
    "bootstrap": "~4.0.0-beta.3",
    "markdown": "0.5.0",
    "vue": "~2.5.13",
    "vue-class-component": "~6.1.2",
    "vue-property-decorator": "~6.0.0",
    "vue-router": "~3.0.1",
    "vuex": "~3.0.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "~0.1.17",
    "copy-webpack-plugin": "~4.3.1",
    "css-loader": "~0.28.7",
    "extract-text-webpack-plugin": "~3.0.2",
    "html-webpack-plugin": "~2.30.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jquery": "~3.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "~0.2.2",
    "node-sass": "~4.7.2",
    "optimize-css-assets-webpack-plugin": "~3.2.0",
    "popper.js": "~1.12.5",
    "postcss": "~6.0.13",
    "standard-version": "~4.2.0",
    "sass-loader": "~6.0.6",
    "style-loader": "~0.19.1",
    "stylelint": "~8.0.0",
    "stylelint-config-standard": "~17.0.0",
    "tslint": "~5.8.0",
    "ts-loader": "~3.2.0",
    "typescript": "~2.6.2",
    "vue-loader": "~13.6.2",
    "vue-template-compiler": "~2.5.13",
    "vue-ts-loader": "0.0.3",
    "webpack": "~3.10.0",
    "webpack-dev-server": "~2.9.7",
    "webpack-merge": "~4.1.1"
  }
